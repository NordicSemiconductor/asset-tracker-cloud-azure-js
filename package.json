{
  "name": "@nordicsemiconductor/asset-tracker-cloud-azure",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using in TypeScript.",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "husky install && check-node-version --package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js/issues"
  },
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@azure/arm-appservice": "9.0.0",
    "@azure/arm-deviceprovisioningservices": "3.0.0",
    "@azure/cosmos": "3.15.0",
    "@azure/functions": "3.0.0",
    "@azure/keyvault-secrets": "4.3.0",
    "@azure/ms-rest-azure-js": "2.1.0",
    "@azure/ms-rest-js": "2.6.0",
    "@azure/ms-rest-nodeauth": "3.1.1",
    "@azure/storage-blob": "12.8.0",
    "@azure/storage-queue": "12.7.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "4.0.1",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "2.0.1",
    "@nordicsemiconductor/object-to-env": "3.0.0",
    "@nordicsemiconductor/random-words": "6.0.0",
    "@octokit/rest": "18.12.0",
    "@sinclair/typebox": "0.23.2",
    "ajv": "8.8.2",
    "azure-iot-provisioning-service": "1.9.0",
    "azure-iothub": "1.14.6",
    "backoff": "2.5.0",
    "mqtt": "4.3.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@azure/data-tables": "13.0.0",
    "@azure/logger": "1.0.3",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.4",
    "@nordicsemiconductor/e2e-bdd-test-runner": "15.0.4",
    "@nordicsemiconductor/firmware-ci-device-helpers": "13.0.5",
    "@types/backoff": "2.5.2",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "27.4.0",
    "@types/jsonwebtoken": "8.5.7",
    "@types/node": "17.0.1",
    "@types/pem": "1.9.6",
    "@types/semver": "7.3.9",
    "@types/uuid": "8.3.4",
    "chai-subset": "1.6.0",
    "chalk": "5.0.0",
    "check-node-version": "4.2.1",
    "commander": "8.3.0",
    "dependency-tree": "^8.1.2",
    "jest": "27.4.7",
    "node-fetch": "3.1.0",
    "pem": "1.14.4",
    "semver": "7.3.5",
    "ts-jest": "27.1.2"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  },
  "deploy": {
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git",
      "branch": "saga"
    }
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
