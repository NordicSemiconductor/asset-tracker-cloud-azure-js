{
  "name": "@nordicsemiconductor/asset-tracker-cloud-azure",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using in TypeScript.",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "husky install && check-node-version --package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js/issues"
  },
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@azure/arm-appservice": "13.0.0",
    "@azure/arm-deviceprovisioningservices": "5.0.1",
    "@azure/cosmos": "3.16.3",
    "@azure/functions": "3.2.0",
    "@azure/keyvault-secrets": "4.4.0",
    "@azure/ms-rest-azure-js": "2.1.0",
    "@azure/ms-rest-js": "2.6.2",
    "@azure/ms-rest-nodeauth": "3.1.1",
    "@azure/storage-blob": "12.11.0",
    "@azure/storage-queue": "12.10.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "5.0.1",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "2.0.8",
    "@nordicsemiconductor/object-to-env": "4.1.1",
    "@nordicsemiconductor/random-words": "6.0.0",
    "@octokit/rest": "19.0.3",
    "@sinclair/typebox": "0.24.26",
    "ajv": "8.11.0",
    "azure-iot-provisioning-service": "1.10.0",
    "azure-iothub": "1.15.0",
    "backoff": "2.5.0",
    "fp-ts": "2.12.2",
    "mqtt": "4.3.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@azure/arm-iothub": "6.1.2",
    "@azure/arm-resources-subscriptions": "2.0.1",
    "@azure/data-tables": "13.1.2",
    "@azure/logger": "1.0.3",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.106",
    "@nordicsemiconductor/e2e-bdd-test-runner": "15.1.67",
    "@nordicsemiconductor/firmware-ci-device-helpers": "13.0.214",
    "@swc/jest": "0.2.22",
    "@types/backoff": "2.5.2",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "28.1.6",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "18.0.0",
    "@types/pem": "1.9.6",
    "@types/semver": "7.3.10",
    "@types/uuid": "8.3.4",
    "chai-subset": "1.6.0",
    "chalk": "5.0.1",
    "check-node-version": "4.2.1",
    "commander": "9.4.0",
    "dependency-tree": "^8.1.2",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "node-fetch": "3.2.10",
    "pem": "1.14.6",
    "semver": "7.3.7"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  },
  "deploy": {
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git",
      "branch": "saga"
    }
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
