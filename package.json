{
  "name": "@nordicsemiconductor/asset-tracker-cloud-azure",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using in TypeScript.",
  "type": "module",
  "main": "./dist/export.js",
  "scripts": {
    "test": "jest",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js/issues"
  },
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@azure/arm-appservice": "13.0.3",
    "@azure/arm-deviceprovisioningservices": "5.0.1",
    "@azure/cosmos": "3.17.2",
    "@azure/functions": "3.5.0",
    "@azure/keyvault-secrets": "4.6.0",
    "@azure/ms-rest-azure-js": "2.1.0",
    "@azure/ms-rest-js": "2.6.4",
    "@azure/ms-rest-nodeauth": "3.1.1",
    "@azure/storage-blob": "12.12.0",
    "@azure/storage-queue": "12.11.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "5.0.2",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "3.0.3",
    "@nordicsemiconductor/object-to-env": "4.1.2",
    "@nordicsemiconductor/random-words": "6.0.1",
    "@octokit/rest": "19.0.5",
    "@sinclair/typebox": "0.25.15",
    "ajv": "8.11.2",
    "azure-iot-provisioning-service": "1.10.1",
    "azure-iothub": "1.16.1",
    "backoff": "2.5.0",
    "mqtt": "4.3.7",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@azure/arm-iothub": "6.2.0",
    "@azure/arm-resources-subscriptions": "2.0.2",
    "@azure/data-tables": "13.2.0",
    "@azure/logger": "1.0.3",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.159",
    "@nordicsemiconductor/e2e-bdd-test-runner": "15.1.189",
    "@nordicsemiconductor/firmware-ci-device-helpers": "13.2.71",
    "@swc/jest": "0.2.24",
    "@types/backoff": "2.5.2",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.17",
    "@types/pem": "1.9.6",
    "@types/semver": "7.3.13",
    "@types/uuid": "8.3.4",
    "chai-subset": "1.6.0",
    "chalk": "5.2.0",
    "check-node-version": "4.2.1",
    "commander": "9.4.1",
    "dependency-tree": "^8.1.2",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "node-fetch": "3.3.0",
    "semver": "7.3.8",
    "tsx": "3.12.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/cli/iot/certificates",
    "dist/cli/process",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  },
  "deploy": {
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git",
      "branch": "saga"
    }
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
