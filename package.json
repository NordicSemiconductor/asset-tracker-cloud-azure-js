{
  "name": "@nordicsemiconductor/asset-tracker-cloud-azure",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using in TypeScript.",
  "scripts": {
    "test": "jest",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-azure-js/issues"
  },
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@azure/arm-appservice": "8.1.1",
    "@azure/arm-deviceprovisioningservices": "2.2.1",
    "@azure/cosmos": "3.13.0",
    "@azure/functions": "1.2.3",
    "@azure/ms-rest-azure-js": "2.1.0",
    "@azure/ms-rest-js": "2.6.0",
    "@azure/ms-rest-nodeauth": "3.0.10",
    "@azure/storage-blob": "12.7.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "3.0.13",
    "@nordicsemiconductor/object-to-env": "2.0.1",
    "@nordicsemiconductor/random-words": "5.0.0",
    "@octokit/rest": "18.9.1",
    "azure-iot-provisioning-service": "1.8.10",
    "azure-iothub": "1.14.3",
    "backoff": "2.5.0",
    "mqtt": "4.2.8",
    "node-fetch": "2.6.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-angular": "13.1.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.4",
    "@nordicsemiconductor/e2e-bdd-test-runner": "14.3.15",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "8.0.7",
    "@nordicsemiconductor/firmware-ci-device-helpers": "9.0.69",
    "@types/backoff": "2.5.2",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "26.0.24",
    "@types/jsonwebtoken": "8.5.4",
    "@types/node": "15.12.5",
    "@types/pem": "1.9.6",
    "@types/semver": "7.3.8",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "chai-subset": "1.6.0",
    "chalk": "4.1.2",
    "commander": "8.1.0",
    "eslint": "7.32.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "pem": "1.14.4",
    "pinst": "2.1.6",
    "prettier": "2.3.2",
    "semver": "7.3.5",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  },
  "deploy": {
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git",
      "branch": "saga"
    }
  }
}
