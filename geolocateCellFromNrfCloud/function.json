{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get"],
      "route": "cellgeolocation/nrfcloud"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "cellGeolocation",
      "type": "cosmosDB",
      "databaseName": "cellGeolocation",
      "containerName": "nrfCloudCache",
      "createIfNotExists": true,
      "partitionKey": "/cellId",
      "connection": "COSMOSDB_CONNECTION_STRING",
      "direction": "out"
    }
  ],
  "scriptFile": "./handler.mjs"
}
