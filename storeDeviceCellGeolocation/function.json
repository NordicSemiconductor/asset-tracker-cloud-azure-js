{
  "bindings": [
    {
      "name": "update",
      "type": "eventHubTrigger",
      "direction": "in",
      "eventHubName": "%IOTHUB_EVENTS_EVENT_HUB_NAME%",
      "cardinality": "one",
      "connection": "IOTHUB_EVENTS_CONNECTION_STRING",
      "consumerGroup": "%CELL_GEOLOCATION_IOT_EVENTS_CONSUMER_GROUP_NAME%"
    },
    {
      "name": "deviceCellGeolocation",
      "type": "cosmosDB",
      "databaseName": "cellGeolocation",
      "containerName": "deviceCellGeolocations",
      "createIfNotExists": true,
      "partitionKey": "/cellId",
      "connection": "COSMOSDB_CONNECTION_STRING",
      "direction": "out"
    }
  ],
  "scriptFile": "./handler.mjs"
}
